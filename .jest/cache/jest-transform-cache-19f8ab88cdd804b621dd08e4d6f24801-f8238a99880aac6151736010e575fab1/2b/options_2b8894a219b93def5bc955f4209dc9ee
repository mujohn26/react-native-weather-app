var assign = require('lodash/assign');

exports.default = {
  withDomLvl1: true,
  normalizeWhitespace: false,
  xml: false,
  decodeEntities: true
};

exports.flatten = function (options) {
  return options && options.xml ? assign({
    xmlMode: true
  }, options.xml) : options;
};
